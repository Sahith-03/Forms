generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model form_schema{
  id Int @id @default(autoincrement())
  field_name String
  field_type String 
  options Json 
  FormData  form_data[]
}

model form_data{
  id Int @id @default(autoincrement())
  formSchema  form_schema @relation(fields: [formSchemaId], references: [id])
  formSchemaId Int
  responses Json
}

